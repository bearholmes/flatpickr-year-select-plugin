    "name": "bearholmes",
    "email": "bearholmes@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bearholmes/flatpickr-year-select-plugin/issues"
  },
  "homepage": "https://github.com/bearholmes/flatpickr-year-select-plugin#readme",
  "peerDependencies": {
    "flatpickr": "^4.6.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.25",
    "@typescript-eslint/eslint-plugin": "^8.22.1",
    "@typescript-eslint/parser": "^8.22.1",
    "autoprefixer": "^10.4.22",
    "cssnano": "^6.1.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "flatpickr": "^4.6.13",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.49",
    "prettier": "^3.7.0",
    "sass": "^1.94.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4"
  }
